-static int eph_new_conn(int sfd, void *func)
-{
-	struct eph_conn *conn = malloc(sizeof(struct eph_conn));
-	struct epoll_event ev;
-
-	if (!conn)
-		return -1;
-
-	memset(conn, 0, sizeof(*conn));
-	DBL_INIT_LIST_HEAD(&conn->lnk);
-	conn->sfd = sfd;
-	conn->events = 0;
-	conn->revents = 0;
-	conn->nbytes = conn->rindex = 0;
